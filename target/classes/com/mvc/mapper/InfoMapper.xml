<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mvc.mapper.InfoMapper">


    <insert id="delivery">
        INSERT INTO info (`id` , `name` , `address`,`phone` , `email` , `number`)
        values(#{info.id},#{info.name},#{info.address},#{info.phone},#{info.email},#{info.number})
    </insert>
</mapper>
